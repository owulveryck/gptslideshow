      }
    }
  },
  {
    "insertText": {
      "insertionIndex": 438,
      "objectId": "SLIDES_API1145746793_1",
      "text": " – Early stages of structuring data and building robust applications.\n"
    }
  },
  {
    "updateTextStyle": {
      "fields": "bold,italic",
      "objectId": "SLIDES_API1145746793_1",
      "style": {},

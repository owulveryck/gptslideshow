      "insertionIndex": 595,
      "objectId": "SLIDES_API1145746793_1",
      "text": "Commodity"
    }
  },
  {
    "updateTextStyle": {
      "fields": "bold,italic",
      "objectId": "SLIDES_API1145746793_1",
      "style": {
        "bold": true

      }
    }
  },
  {
    "insertText": {
      "insertionIndex": 341,
      "objectId": "SLIDES_API1145746793_1",
      "text": "Genesis"
    }
  },
  {
    "updateTextStyle": {
      "fields": "bold,italic",
      "objectId": "SLIDES_API1145746793_1",
      "style": {
        "bold": true

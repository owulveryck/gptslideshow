      }
    }
  },
  {
    "insertText": {
      "insertionIndex": 348,
      "objectId": "SLIDES_API1145746793_1",
      "text": " – Where data is initially unstructured, akin to the experimental world of startups.\n"
    }
  },
  {
    "updateTextStyle": {
      "fields": "bold,italic",
      "objectId": "SLIDES_API1145746793_1",
      "style": {},
